
package puzzle;

import java.lang.reflect.*;

public class Puzzle {
    
    final int p = 24;
    int v;
    private static int[] arrayNums;
    static boolean b=false;
                
    public void setNums() {
        int[] n = new int[4];
        n[0]= 4;
        n[1]= 1;
        n[2]= 8;
        n[3]= 7;
        arrayNums = n;
    }
            
    public static void main(String[] args) 
            throws IllegalAccessException, IllegalArgumentException, InvocationTargetException {
        Puzzle s = new Puzzle();
        s.setNums();
        s.canBeEqualTo24(arrayNums);
        System.out.println(b);
    }
    
    public boolean canBeEqualTo24(int[] nums) 
    throws IllegalAccessException, IllegalArgumentException, InvocationTargetException {
        Puzzle s = new Puzzle();        
        Sequence seq = new Sequence();
        Class c1 = seq.getClass();
        
        Method[] m1 = Sequence.class.getMethods();
               
        for(int i = 0; i < m1.length; i++) {
            int[] result1 = (int[]) m1[i].invoke(seq, nums);
            String MethodName = m1[i].getName();
            
            float[] r = new float[4];
            r[0] =  result1[0]; 
            r[1] =  result1[1];
            r[2] =  result1[2];
            r[3] =  result1[3];
            //System.out.println(MethodName);
            
            s.all(r);
            System.out.println(MethodName);
            
        }
             
        return b;
      
    }
 
