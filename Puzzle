            cmb[j++]= a[0]-(a[1]/(a[2]+a[3]));
            cmb[j++]= a[0]-(a[1]/(a[2]-a[3])); 
            cmb[j++]= a[0]-(a[1]/(a[2]*a[3]));
            cmb[j++]= a[0]-(a[1]/(a[2]/a[3])); 
   
            cmb[j++]= a[0]*(a[1]+(a[2]+a[3]));
            cmb[j++]= a[0]*(a[1]+(a[2]-a[3])); 
            cmb[j++]= a[0]*(a[1]+(a[2]*a[3]));
            cmb[j++]= a[0]*(a[1]+(a[2]/a[3]));
            cmb[j++]= a[0]*(a[1]-(a[2]+a[3]));
            cmb[j++]= a[0]*(a[1]-(a[2]-a[3])); 
            cmb[j++]= a[0]*(a[1]-(a[2]*a[3]));
            cmb[j++]= a[0]*(a[1]-(a[2]/a[3]));
            cmb[j++]= a[0]*(a[1]*(a[2]+a[3]));
            cmb[j++]= a[0]*(a[1]*(a[2]-a[3])); 
            cmb[j++]= a[0]*(a[1]*(a[2]*a[3]));
            cmb[j++]= a[0]*(a[1]*(a[2]/a[3]));
            cmb[j++]= a[0]*(a[1]/(a[2]+a[3]));
            cmb[j++]= a[0]*(a[1]/(a[2]-a[3])); 
            cmb[j++]= a[0]*(a[1]/(a[2]*a[3]));
            cmb[j++]= a[0]*(a[1]/(a[2]/a[3]));
    
            cmb[j++]= a[0]/(a[1]+(a[2]+a[3]));
            cmb[j++]= a[0]/(a[1]+(a[2]-a[3])); 
            cmb[j++]= a[0]/(a[1]+(a[2]*a[3]));
            cmb[j++]= a[0]/(a[1]+(a[2]/a[3]));
            cmb[j++]= a[0]/(a[1]-(a[2]+a[3]));
            cmb[j++]= a[0]/(a[1]-(a[2]-a[3])); 
            cmb[j++]= a[0]/(a[1]-(a[2]*a[3]));
            cmb[j++]= a[0]/(a[1]-(a[2]/a[3]));
            cmb[j++]= a[0]/(a[1]*(a[2]+a[3]));
            cmb[j++]= a[0]/(a[1]*(a[2]-a[3])); 
            cmb[j++]= a[0]/(a[1]*(a[2]*a[3]));
            cmb[j++]= a[0]/(a[1]*(a[2]/a[3]));
            cmb[j++]= a[0]/(a[1]/(a[2]+a[3]));
            cmb[j++]= a[0]/(a[1]/(a[2]-a[3])); 
            cmb[j++]= a[0]/(a[1]/(a[2]*a[3]));
            cmb[j++]= a[0]/(a[1]/(a[2]/a[3]));
            
        for (int i = 0; i < cmb.length; i++){
            v = Math.round(cmb[i]);
            if (v==24) {
                System.out.println("cmb{"+i+"]");
                break;
            }    
        }
    return b;
    } 
}
