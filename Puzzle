        cmb[j++]= a[0]-(a[1]+(a[2]+a[3]));
        cmb[j++]= a[0]-(a[1]+(a[2]-a[3])); 
        cmb[j++]= a[0]-(a[1]+(a[2]*a[3]));
        cmb[j++]= a[0]-(a[1]+(a[2]/a[3]));
        cmb[j++]= a[0]-(a[1]-(a[2]+a[3]));
        cmb[j++]= a[0]-(a[1]-(a[2]-a[3])); 
        cmb[j++]= a[0]-(a[1]-(a[2]*a[3]));
        cmb[j++]= a[0]-(a[1]-(a[2]/a[3]));
        cmb[j++]= a[0]-(a[1]*(a[2]+a[3]));
        cmb[j++]= a[0]-(a[1]*(a[2]-a[3]));
        cmb[j++]= a[0]-(a[1]*(a[2]*a[3])); 
        cmb[j++]= a[0]-(a[1]*(a[2]/a[3]));
        cmb[j++]= a[0]-(a[1]/(a[2]+a[3]));
        cmb[j++]= a[0]-(a[1]/(a[2]-a[3])); 
        cmb[j++]= a[0]-(a[1]/(a[2]*a[3]));
        cmb[j++]= a[0]-(a[1]/(a[2]/a[3])); 
   
        cmb[j++]= a[0]*(a[1]+(a[2]+a[3]));
        cmb[j++]= a[0]*(a[1]+(a[2]-a[3])); 
        cmb[j++]= a[0]*(a[1]+(a[2]*a[3]));
        cmb[j++]= a[0]*(a[1]+(a[2]/a[3]));
        cmb[j++]= a[0]*(a[1]-(a[2]+a[3]));
        cmb[j++]= a[0]*(a[1]-(a[2]-a[3])); 
        cmb[j++]= a[0]*(a[1]-(a[2]*a[3]));
        cmb[j++]= a[0]*(a[1]-(a[2]/a[3]));
        cmb[j++]= a[0]*(a[1]*(a[2]+a[3]));
        cmb[j++]= a[0]*(a[1]*(a[2]-a[3])); 
        cmb[j++]= a[0]*(a[1]*(a[2]*a[3]));
        cmb[j++]= a[0]*(a[1]*(a[2]/a[3]));
        cmb[j++]= a[0]*(a[1]/(a[2]+a[3]));
        cmb[j++]= a[0]*(a[1]/(a[2]-a[3])); 
        cmb[j++]= a[0]*(a[1]/(a[2]*a[3]));
        cmb[j++]= a[0]*(a[1]/(a[2]/a[3]));
    
        cmb[j++]= a[0]/(a[1]+(a[2]+a[3]));
        cmb[j++]= a[0]/(a[1]+(a[2]-a[3])); 
        cmb[j++]= a[0]/(a[1]+(a[2]*a[3]));
        cmb[j++]= a[0]/(a[1]+(a[2]/a[3]));
        cmb[j++]= a[0]/(a[1]-(a[2]+a[3]));
        cmb[j++]= a[0]/(a[1]-(a[2]-a[3])); 
        cmb[j++]= a[0]/(a[1]-(a[2]*a[3]));
        cmb[j++]= a[0]/(a[1]-(a[2]/a[3]));
        cmb[j++]= a[0]/(a[1]*(a[2]+a[3]));
        cmb[j++]= a[0]/(a[1]*(a[2]-a[3])); 
        cmb[j++]= a[0]/(a[1]*(a[2]*a[3]));
        cmb[j++]= a[0]/(a[1]*(a[2]/a[3]));
        cmb[j++]= a[0]/(a[1]/(a[2]+a[3]));
        cmb[j++]= a[0]/(a[1]/(a[2]-a[3])); 
        cmb[j++]= a[0]/(a[1]/(a[2]*a[3]));
        cmb[j++]= a[0]/(a[1]/(a[2]/a[3]));
            
        for (int i = 0; i < cmb.length; i++){
            v = i;
            if (Math.round(cmb[i])==p) {
                b = true;
                //System.out.println("cmb["+i+"]" + b);
                break;
            }    
        }
    return b;
    } 
}
