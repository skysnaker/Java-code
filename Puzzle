
package puzzle;

import java.lang.reflect.*;
import java.util.HashMap; 
import java.util.Map; 

public class Puzzle {
    
    final int p = 24;
    static int v;
    private static int[] arrayNums;
    static boolean b;
    static String met;
    static String formula;
                
    public void setNums() {
        int[] n = new int[4];
        n[0]= 4;
        n[1]= 1;
        n[2]= 8;
        n[3]= 7;
        arrayNums = n;
    }
            
    public static void main(String[] args) 
            throws IllegalAccessException, IllegalArgumentException, InvocationTargetException {
        Puzzle s = new Puzzle();
        s.setNums();
        s.canBeEqualTo24(arrayNums);
        System.out.println(b +" method_"+ met+ " combination_â„–"+v );
        System.out.println("formula: "+ formula);
    }
    
    public boolean canBeEqualTo24(int[] nums) 
    throws IllegalAccessException, IllegalArgumentException, InvocationTargetException {
        Puzzle s = new Puzzle();        
        Sequence seq = new Sequence();
        float[] r = new float[4];
            r[0] =  nums[0]; 
            r[1] =  nums[1];
            r[2] =  nums[2];
            r[3] =  nums[3];
                
        Method[] m = Sequence.class.getMethods();
               
        for (Method m1 : m) {
            float[] result = (float[]) m1.invoke(seq, r);
            String MethodName = m1.getName();
            
            s.all(result);            //method of combination
            if (b==true){
                met = MethodName;
                break;
            }
        }
             
        return b;
      
    }
 
    public boolean all (float[]a){
        int j = 0;
        int g = 0;
        float[] cmb = new float [320];
        
        Map<Integer, String> hashMap = new HashMap<Integer, String>();
        hashMap.put((int)(a[0]+a[1]+a[2]+a[3]), "a[0]+a[1]+a[2]+a[3]");
        hashMap.put((int)(a[0]+a[2]+a[2]+a[3]), "a[0]+a[1]+a[2]+a[3]");
        hashMap.put((int)(a[0]+a[2]+a[2]+a[3]), "a[0]+a[1]+a[2]+a[3]");
        hashMap.put((int)(a[0]+a[0]+a[2]+a[3]), "a[0]+a[1]+a[2]+a[3]");
        hashMap.put((int)(a[0]+a[3]+a[2]+a[3]), "a[0]+a[1]+a[2]+a[3]");
        
        for (Integer key : hashMap.keySet()) {
            if (key>2) {
                b = true;
                formula = hashMap.get(key);
                break;
            } else {
                b = false;
            }
            
        }
        
        // a-combination x_a[1]_a[2]_a[3] 64
        cmb[j++]= a[0]+a[1]+a[2]+a[3];
        cmb[j++]= a[0]+a[1]+a[2]-a[3]; 
        cmb[j++]= a[0]+a[1]+a[2]*a[3];
