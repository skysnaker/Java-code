package puzzle;

public class Sequence {
    
        //first s[0]
        
        public int[] seq1 (int[] s){ int[] ar = new int[4];
            ar[0]=s[0]; ar[1]=s[1]; ar[2]=s[2]; ar[3]=s[3]; return ar;}
        
